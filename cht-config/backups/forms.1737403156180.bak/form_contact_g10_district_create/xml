      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/create_new_person" type="select1" relevant=" /data/init/place_type  != 'g40_clinic'" required="true()"/>
      <bind nodeset="/data/init/select_person" type="db:person" relevant="selected( /data/init/create_new_person ,'old_person')" required="true()"/>
      <bind nodeset="/data/init/name" type="string"/>
      <bind nodeset="/data/init/contact_name" type="string" calculate="coalesce(../name, ../../contact/name)"/>
      <bind nodeset="/data/init/custom_place_name_label_translator" type="select1" relevant="selected( /data/init/create_new_person ,'none')" calculate="&quot;name&quot;"/>
      <bind nodeset="/data/init/custom_place_name_label" type="string" calculate="jr:choice-name( /data/init/custom_place_name_label_translator ,' /data/init/custom_place_name_label_translator ')"/>
      <bind nodeset="/data/init/custom_place_name" type="string" relevant="selected( /data/init/create_new_person ,'none') or not(selected( /data/g10_district/is_name_generated , 'true'))" required="true()"/>
      <bind nodeset="/data/init/place_type" type="select1"/>
      <bind nodeset="/data/init/place_type_translation" type="string" calculate="jr:choice-name( /data/init/place_type ,' /data/init/place_type ')"/>
      <bind nodeset="/data/init/generated_name" type="select1"/>
      <bind nodeset="/data/init/generated_name_translation" type="string" calculate="jr:choice-name( /data/init/generated_name ,' /data/init/generated_name ')"/>
      <bind nodeset="/data/contact" relevant="selected( /data/init/create_new_person ,'new_person') or  /data/init/place_type  = 'g40_clinic'"/>
      <bind nodeset="/data/contact/parent" type="string"/>
      <bind nodeset="/data/contact/type" type="string"/>
      <bind nodeset="/data/contact/role" type="select1" calculate="if( /data/init/place_type  = 'g40_clinic', 'chw')" required="true()"/>
      <bind nodeset="/data/contact/first_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/contact/first_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/last_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/contact/last_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/name" type="string" calculate="concat( /data/contact/first_name , ' ',  /data/contact/last_name )"/>
      <bind nodeset="/data/contact/phone" type="tel" constraint="true()" jr:constraintMsg="jr:itext('/data/contact/phone:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/phone_alternate" type="tel" constraint="true()" jr:constraintMsg="jr:itext('/data/contact/phone_alternate:jr:constraintMsg')"/>
      <bind nodeset="/data/contact/sex" type="select1" relevant=" /data/contact/role  != 'chw'" required=" /data/contact/role  = 'patient'"/>
      <bind nodeset="/data/contact/caste_code" type="select" constraint="not(selected(., 'unknown') and (selected(., 'dalit') or selected(., 'janajati') or selected(., 'madhesi')or selected(., 'muslim')or selected(., 'brahmin_chettri') or selected(., 'other') ))" jr:constraintMsg="jr:itext('/data/contact/caste_code:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/year_of_birth_in_bikram_sambat" type="string" constraint="regex(.,'^[0-9]{4}$') and (int(format-date(today(),'%Y')) + 57 - 100) &lt;= . and (int(format-date(today(),'%Y')) + 57 - 10) &gt;= ." jr:constraintMsg="jr:itext('/data/contact/year_of_birth_in_bikram_sambat:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/year_of_birth_approx" type="string" relevant=" /data/contact/role  = 'chw' and  /data/contact/year_of_birth_in_bikram_sambat  != ''" calculate="int( /data/contact/year_of_birth_in_bikram_sambat ) - 57"/>
      <bind nodeset="/data/contact/date_of_birth" type="date" relevant=" /data/contact/role  != 'chw'"/>
      <bind nodeset="/data/contact/fchv_id_number" type="string" constraint="regex(.,'^[0-9]*$')" jr:constraintMsg="jr:itext('/data/contact/fchv_id_number:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/start_date_as_fchv" type="string" constraint="regex(.,'^[0-9]{4}$') and int(.)&gt;= 2044 and int(format-date(today(),'%Y')) + 57 &gt;= int(.) and int(.) &gt; int( /data/contact/year_of_birth_in_bikram_sambat  ) + 9" jr:constraintMsg="jr:itext('/data/contact/start_date_as_fchv:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/start_date_as_fchv_ad" type="string" relevant=" /data/contact/role  = 'chw'" calculate="int( /data/contact/start_date_as_fchv ) - 57"/>
      <bind nodeset="/data/contact/literate" type="select1" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/level_of_education" type="select1" relevant=" /data/contact/role  = 'chw' and  /data/contact/literate ='true'"/>
      <bind nodeset="/data/contact/basic_training" type="select1" relevant=" /data/contact/role  = 'chw'"/>
      <bind nodeset="/data/contact/training_date" type="date" constraint="int(format-date(.,'%Y')) &gt;= 1987 and int(format-date(.,'%Y')) &gt; int(int( /data/contact/year_of_birth_in_bikram_sambat  - 57)) and int(format-date(.,'%Y')) &gt;= int(int( /data/contact/start_date_as_fchv  - 57)) and . &lt; today()" jr:constraintMsg="jr:itext('/data/contact/training_date:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw' and  /data/contact/basic_training ='true'"/>
      <bind nodeset="/data/contact/most_recent_fchv_training" type="date" constraint="int(format-date(.,'%Y')) &gt;= 1990 and int(format-date(.,'%Y')) &gt; int(int( /data/contact/year_of_birth_in_bikram_sambat  - 57)) and int(format-date(.,'%Y')) &gt;= int(int( /data/contact/start_date_as_fchv  - 57)) and . &lt; today() and . &gt;  /data/contact/training_date " jr:constraintMsg="jr:itext('/data/contact/most_recent_fchv_training:jr:constraintMsg')" relevant=" /data/contact/role  = 'chw' and  /data/contact/basic_training ='true'"/>
      <bind nodeset="/data/contact/external_id" type="string"/>
      <bind nodeset="/data/contact/notes" type="string" constraint="string-length(.)&lt;=100" jr:constraintMsg="jr:itext('/data/contact/notes:jr:constraintMsg')"/>
      <bind nodeset="/data/contact/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/contact/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/contact/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/g10_district/parent" type="string"/>
      <bind nodeset="/data/g10_district/type" type="string"/>
      <bind nodeset="/data/g10_district/is_name_generated" type="select1" relevant="not(selected( /data/init/create_new_person ,'none')) and  /data/init/place_type  = 'g40_clinic'" required="true()"/>
      <bind nodeset="/data/g10_district/name" type="string" calculate="if(  /data/g10_district/is_name_generated  = 'true',  /data/init/generated_name_translation ,  /data/init/custom_place_name )" required="true()"/>
      <bind nodeset="/data/g10_district/external_id" type="string" relevant="false()"/>
      <bind nodeset="/data/g10_district/notes" type="string"/>
      <bind nodeset="/data/g10_district/contact" type="string" calculate="if(selected( /data/init/create_new_person ,'none'), &quot;&quot;, coalesce( /data/init/select_person ,&quot;NEW&quot;))"/>
      <bind nodeset="/data/g10_district/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))"/>
      <bind nodeset="/data/g10_district/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/g10_district/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/g10_district/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
