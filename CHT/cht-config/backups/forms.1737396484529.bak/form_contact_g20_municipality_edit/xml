      <bind nodeset="/data/inputs/user" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/g20_municipality/parent" type="string" readonly="true()"/>
      <bind nodeset="/data/g20_municipality/type" type="string" readonly="true()"/>
      <bind nodeset="/data/g20_municipality/contact/name" type="string"/>
      <bind nodeset="/data/g20_municipality/contact/contact_name" type="string" calculate="../name"/>
      <bind nodeset="/data/g20_municipality/contact/_id" type="string" constraint=" /data/g20_municipality/contact/role  = 'chw' or  /data/g20_municipality/contact/role  = 'other'" jr:constraintMsg="jr:itext('/data/g20_municipality/contact/_id:jr:constraintMsg')"/>
      <bind nodeset="/data/g20_municipality/contact/role" type="string"/>
      <bind nodeset="/data/g20_municipality/is_name_generated" type="select1" calculate="if(  /data/g20_municipality/generated_name  = ../name, 'true', 'false')" required="true()" readonly="boolean( /data/g20_municipality/contact/_id )" relevant="not( /data/g20_municipality/contact/_id ) or selected( . , 'true') or boolean( /data/g20_municipality/contact/_id )"/>
      <bind nodeset="/data/g20_municipality/generated_name" type="string" calculate=" /data/init/generated_name_translation "/>
      <bind nodeset="/data/g20_municipality/name" type="string" calculate="if(  /data/g20_municipality/is_name_generated  = 'true',  /data/g20_municipality/generated_name , .)" required="true()" relevant="boolean( /data/g20_municipality/contact/_id ) or not( /data/g20_municipality/contact/_id ) or not(selected( /data/g20_municipality/is_name_generated , 'true'))"/>
      <bind nodeset="/data/g20_municipality/external_id" type="string"/>
      <bind nodeset="/data/g20_municipality/notes" type="string"/>
      <bind nodeset="/data/g20_municipality/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))"/>
      <bind nodeset="/data/g20_municipality/meta/created_by" type="string" readonly="true()"/>
      <bind nodeset="/data/g20_municipality/meta/created_by_person_uuid" type="string" readonly="true()"/>
      <bind nodeset="/data/g20_municipality/meta/created_by_place_uuid" type="string"/>
      <bind nodeset="/data/g20_municipality/meta/last_edited_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/g20_municipality/meta/last_edited_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/g20_municipality/meta/last_edited_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/init/place_type" type="select1" calculate="&quot;g20_municipality&quot;"/>
      <bind nodeset="/data/init/place_type_translation" type="string" calculate="jr:choice-name( /data/init/place_type ,' /data/init/place_type ')"/>
      <bind nodeset="/data/init/select_generated_name" type="select1" calculate="&quot;g20_municipality&quot;"/>
      <bind nodeset="/data/init/generated_name_translation" type="string" calculate="if (boolean( /data/g20_municipality/contact/_id ), jr:choice-name( /data/init/select_generated_name ,' /data/init/select_generated_name '), '')"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
