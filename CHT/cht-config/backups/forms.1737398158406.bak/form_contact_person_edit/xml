      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/person_name" type="string" calculate="../../person/name"/>
      <bind nodeset="/data/init/person_role" type="string" calculate="../../person/first_name"/>
      <bind nodeset="/data/init/person_place_name" type="string" calculate="../name"/>
      <bind nodeset="/data/init/person_parent" type="db:district_hospital" readonly="true()" calculate=" /data/person/parent "/>
      <bind nodeset="/data/init/contact_type" type="string"/>
      <bind nodeset="/data/init/type_selector" type="select1" calculate="../contact_type"/>
      <bind nodeset="/data/init/type" type="string"/>
      <bind nodeset="/data/init/name" type="string"/>
      <bind nodeset="/data/init/note_place" readonly="true()" type="string"/>
      <bind nodeset="/data/person/parent" type="string"/>
      <bind nodeset="/data/person/type" type="string"/>
      <bind nodeset="/data/person/role" type="select1" required="true()"/>
      <bind nodeset="/data/person/first_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/person/first_name:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/person/last_name" type="string" constraint="string-length(.)&lt;=30 and regex(.,'^([^0-9]*)$')" jr:constraintMsg="jr:itext('/data/person/last_name:jr:constraintMsg')"/>
      <bind nodeset="/data/person/name" type="string" calculate="if( /data/person/last_name  = ' ',  /data/person/first_name , concat( /data/person/first_name , ' ',  /data/person/last_name ))"/>
      <bind nodeset="/data/person/phone" type="tel" constraint="true()" jr:constraintMsg="jr:itext('/data/person/phone:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/person/phone_alternate" type="tel" constraint="true()" jr:constraintMsg="jr:itext('/data/person/phone_alternate:jr:constraintMsg')" relevant=" /data/person/role  != 'patient'"/>
      <bind nodeset="/data/person/sex" type="select1" required=" /data/person/role  = 'patient'" relevant=" /data/person/role  != 'chw'"/>
      <bind nodeset="/data/person/caste_code" type="select" constraint="not(selected(., 'unknown') and (selected(., 'dalit') or selected(., 'janajati') or selected(., 'madhesi')or selected(., 'muslim')or selected(., 'brahmin_chettri') or selected(., 'other') ))" jr:constraintMsg="jr:itext('/data/person/caste_code:jr:constraintMsg')" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/year_of_birth_in_bikram_sambat" type="string" constraint="regex(.,'^[0-9]{4}$') and (int(format-date(today(),'%Y')) + 57 - 100) &lt;= . and (int(format-date(today(),'%Y')) + 57 - 10) &gt;= ." jr:constraintMsg="jr:itext('/data/person/year_of_birth_in_bikram_sambat:jr:constraintMsg')" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/year_of_birth_approx" type="string" calculate="int( /data/person/year_of_birth_in_bikram_sambat ) - 57" relevant=" /data/person/role  = 'chw' and  /data/person/year_of_birth_in_bikram_sambat  != ''"/>
      <bind nodeset="/data/person/dob_known" type="select1" required="true()" relevant=" /data/person/role  != 'chw'"/>
      <bind nodeset="/data/person/dob" type="date" required=" /data/person/role  = 'patient'" relevant=" /data/person/role  != 'chw' and  /data/person/dob_known  = 'true'"/>
      <bind nodeset="/data/person/age" type="int" constraint=". &gt;= 10 and . &lt;= 100" required=" /data/person/role  = 'patient'" jr:constraintMsg="jr:itext('/data/person/age:jr:constraintMsg')" relevant=" /data/person/role  != 'chw' and  /data/person/dob_known  = 'false'"/>
      <bind nodeset="/data/person/dob_approx" type="string" calculate="concat(string(int(format-date-time(today(),&quot;%Y&quot;)) - int( /data/person/age )), &quot;-&quot;, string(format-date-time(today(), &quot;%m-%d&quot;)))" required=" /data/person/role  = 'patient'" relevant=" /data/person/age  != ''"/>
      <bind nodeset="/data/person/date_of_birth" type="string" calculate="coalesce(coalesce( /data/person/dob ,  /data/person/dob_approx ), concat( /data/person/year_of_birth_approx , &quot;-01-01&quot;))"/>
      <bind nodeset="/data/person/fchv_id_number" type="string" constraint="regex(.,'^[0-9]*$')" jr:constraintMsg="jr:itext('/data/person/fchv_id_number:jr:constraintMsg')" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/start_date_as_fchv" type="string" constraint="regex(.,'^[0-9]{4}$') and int(.)&gt;= 2044 and int(format-date(today(),'%Y')) + 57 &gt;= int(.) and int(.) &gt; int( /data/person/year_of_birth_in_bikram_sambat  ) + 9" jr:constraintMsg="jr:itext('/data/person/start_date_as_fchv:jr:constraintMsg')" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/start_date_as_fchv_ad" type="string" calculate="int( /data/person/start_date_as_fchv ) - 57" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/literate" type="select1" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/level_of_education" type="select1" relevant=" /data/person/role  = 'chw' and  /data/person/literate ='true'"/>
      <bind nodeset="/data/person/basic_training" type="select1" relevant=" /data/person/role  = 'chw'"/>
      <bind nodeset="/data/person/training_date" type="date" constraint="int(format-date(.,'%Y')) &gt;= 1987 and int(format-date(.,'%Y')) &gt; int(int( /data/person/year_of_birth_in_bikram_sambat  - 57)) and int(format-date(.,'%Y')) &gt;= int(int( /data/person/start_date_as_fchv  - 57)) and . &lt; today()" jr:constraintMsg="jr:itext('/data/person/training_date:jr:constraintMsg')" relevant=" /data/person/role  = 'chw' and  /data/person/basic_training ='true'"/>
      <bind nodeset="/data/person/most_recent_fchv_training" type="date" constraint="int(format-date(.,'%Y')) &gt;= 1990 and int(format-date(.,'%Y')) &gt; int(int( /data/person/year_of_birth_in_bikram_sambat  - 57)) and int(format-date(.,'%Y')) &gt;= int(int( /data/person/start_date_as_fchv  - 57)) and . &lt; today() and . &gt;  /data/person/training_date " jr:constraintMsg="jr:itext('/data/person/most_recent_fchv_training:jr:constraintMsg')" relevant=" /data/person/role  = 'chw' and  /data/person/basic_training ='true'"/>
      <bind nodeset="/data/person/external_id" type="string"/>
      <bind nodeset="/data/person/notes" type="string" constraint="string-length(.)&lt;=100" jr:constraintMsg="jr:itext('/data/person/notes:jr:constraintMsg')"/>
      <bind nodeset="/data/person/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/person/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/person/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
